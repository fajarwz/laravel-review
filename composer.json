{
    "name": "fajarwz/laravel-review",
    "description": "Flexible and powerful review system for Laravel, let any model review and be reviewed.",
    "keywords": [
        "fajarwz",
        "laravel",
        "laravel-review",
        "review",
        "rating"
    ],
    "homepage": "https://github.com/fajarwz/laravel-review",
    "license": "MIT",
    "authors": [
        {
            "name": "fajarwz",
            "email": "hi@fajarwz.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fajarwz\\LaravelReview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fajarwz\\LaravelReview\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fajarwz\\LaravelReview\\LaravelReviewServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^9.2"
    }
}
